{
  "name": "capacitor-video-player",
  "version": "2.0.1",
  "description": "Capacitor Video Player Plugin",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc && npm run copy:hls",
    "copy:hls": "cp -r src/web-utils/hls dist/esm/web-utils",
    "clean": "rm -rf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "deploy": "npm publish"
  },
  "author": "Jean Pierre Qu√©au",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "^2.0.1",
    "@types/hls.js": "^0.12.6",
    "hls.js": "^0.13.2"
  },
  "devDependencies": {
    "@capacitor/android": "^2.0.1",
    "@capacitor/ios": "^2.0.1",
    "rimraf": "^3.0.0",
    "typescript": "^3.2.4"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorVideoPlayer.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "video",
    "video player"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jepiqueau/capacitor-video-player.git"
  },
  "bugs": {
    "url": "https://github.com/jepiqueau/capacitor-video-player.git/issues"
  }
}
